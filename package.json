    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.3.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-playwright": "^1.8.3",
