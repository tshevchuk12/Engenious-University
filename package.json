  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test-ui": "playwright test --ui",
    "testAllWithAllureReporter": "(rimraf allure-results allure-report && playwright test loginPage.spec.ts APItest --reporter=list,allure-playwright || true) && npm run report",
    "report": "npx allure generate ./allure-results --clean && npx allure open",
    "report:generate": "npx allure generate ./allure-results --clean -o ./allure-report",
    "LoginPage": "playwright test loginPage.spec.ts",
    "LoginPage-ui": "playwright test --ui loginPage.spec.ts",
    "APItest": "playwright test APItest",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lintFix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write ."
  },
  "keywords": [],
